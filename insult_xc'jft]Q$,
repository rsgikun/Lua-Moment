tick - 1) end function on_send_packet(id) if id == 0x02 and player.kill_aura_target() ~= 0 and entity.hurt_time(player.kill_aura_target()) >= 9 then OnAttack = true end end function on_chat(msg) if utf8_sub(msg,1,1) == "." then local command = split(utf8_sub(msg,2)," ") if command[1] == "insult_config" then msg = ".kick a" rh:config_chatcheck(json_config,command,getName()) end end return msg end end
